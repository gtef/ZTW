@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<br>
<h2>Wybierz grupę</h2>
<br>
<!--twoje grupy-->
<div class="panel panel-default ">
    <div class="panel-heading">
        <h3>Twoje grupy</h3>
    </div>
    <div class="panel-body">
        <div class="list-group">
            @Html.ActionLink("Grzybiarze", "projekty", "", new { @class = "list-group-item" })
            @Html.ActionLink("Modelarze", "#", "", new { @class = "list-group-item" })
            @Html.ActionLink("Informatycy", "#","" ,new { @class = "list-group-item" })
        </div>
    </div>
</div>

<!--wyszukaj grupę-->
<div class="panel panel-default ">
    <div class="panel-heading">
        <h3>Wyszukaj grupę</h3>
    </div>
    <div class="panel-body">
        <form class="form-horizontal">
            <div class="form-group">
                <label for="searchField" class="col-xs-1 ">hasło</label>
                <div class="col-xs-4">
                    <input type="text" class="form-control" id="searchField" placeholder="nazwa grupy" />
                </div>
            </div>
            <button type="submit" class="btn btn-primary ">Znajdź</button>
        </form>
    </div>
</div>